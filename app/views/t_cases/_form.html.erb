<%= form_for([@project, @project.t_cases.build]) do |f| %>
  <p>
    <%= f.label :t_case_id %><br>
    <%= f.text_field :t_case_id %>
  </p>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :scenario %><br>
    <%= f.text_area :scenario %>
  </p>
  <p>
    <%= f.label :status %><br>
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
